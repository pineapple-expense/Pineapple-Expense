{
    "Comment": "Delete S3 object and invoke Lambda",
    "StartAt": "DeleteObject",
    "States": {
        "DeleteObject": {
            "Type": "Task",
            "Resource": "arn:aws:states:::aws-sdk:s3:deleteObject",
            "Parameters": {
                "Bucket.$": "$.bucket",
                "Key.$": "$.key"
            },
            "ResultPath": null,
            "Next": "Parallel"
        },
        "Parallel": {
            "Type": "Parallel",
            "Branches": [
                {
                    "StartAt": "DeleteReceiptFromReceiptPred",
                    "States": {
                        "DeleteReceiptFromReceiptPred": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::lambda:invoke",
                            "OutputPath": "$.Payload",
                            "Parameters": {
                                "Payload.$": "$",
                                "FunctionName": "arn:aws:lambda:us-east-1:418295723137:function:DeleteReceiptFromReceiptPred:$LATEST"
                            },
                            "Retry": [
                                {
                                    "ErrorEquals": [
                                        "Lambda.ServiceException",
                                        "Lambda.AWSLambdaException",
                                        "Lambda.SdkClientException",
                                        "Lambda.TooManyRequestsException"
                                    ],
                                    "IntervalSeconds": 1,
                                    "MaxAttempts": 3,
                                    "BackoffRate": 2,
                                    "JitterStrategy": "FULL"
                                }
                            ],
                            "End": true
                        }
                    }
                },
                {
                    "StartAt": "DeleteReceiptFromReceiptData",
                    "States": {
                        "DeleteReceiptFromReceiptData": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::lambda:invoke",
                            "Parameters": {
                                "FunctionName": "arn:aws:lambda:us-east-1:418295723137:function:DeleteReceiptFromReceiptData:$LATEST",
                                "Payload.$": "$"
                            },
                            "Retry": [
                                {
                                    "ErrorEquals": [
                                        "Lambda.ServiceException",
                                        "Lambda.AWSLambdaException",
                                        "Lambda.SdkClientException",
                                        "Lambda.TooManyRequestsException"
                                    ],
                                    "IntervalSeconds": 1,
                                    "MaxAttempts": 3,
                                    "BackoffRate": 2,
                                    "JitterStrategy": "FULL"
                                }
                            ],
                            "End": true,
                            "OutputPath": "$.Payload"
                        }
                    }
                }
            ],
            "End": true
        }
    }
}